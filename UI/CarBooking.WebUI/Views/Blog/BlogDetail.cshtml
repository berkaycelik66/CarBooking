
@{
    ViewData["Title"] = "BlogDetail";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<section class="ftco-section ftco-degree-bg">
    <div class="container">
        <div class="row">
            @await Component.InvokeAsync("_BlogDetailMainComponentPartial")
            
            <div class="col-md-4 sidebar ftco-animate">
                
                @await Component.InvokeAsync("_BlogDetailSideBarSearchBoxComponentPartial")

                @await Component.InvokeAsync("_BlogDetailSideBarCategoriesComponentPartial")

                @await Component.InvokeAsync("_BlogDetailSideBarRecentBlogsComponentPartial")

                @await Component.InvokeAsync("_BlogDetailSideBarTagCloudComponentPartial")

                @await Component.InvokeAsync("_BlogDetailSideBarParagraphComponentPartial")               
            </div>

        </div>
    </div>
</section>

